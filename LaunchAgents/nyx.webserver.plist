<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
   <dict>
      <key>Label</key><string>nyx.webserver</string>
      <key>Disabled</key><false/>
      <key>RunAtLoad</key><true/>
      <key>KeepAlive</key><false/>
      <key>ProgramArguments</key>
      <array>
         <string>/nix/var/nix/profiles/per-user/viny/webserver/bin/nginx</string>
         <string>-c</string>
         <string>/Users/viny/.nyx/etc/nginx/nginx.conf</string>
         <string>-g</string>
         <string>daemon off;</string>
      </array>
      <key>StandardErrorPath</key><string>/Users/viny/Library/Logs/nyx/webserver/nginx/stderr.log</string>
   </dict>
</plist>